number=0
x=0
y=0
while x<=3:
    #print(x)
    print(f'({x},{y})')
    number=number+1
    #print(number)
    y=y+1
    if number==4 or number==8 or number==12 or number==16:
        x=x+1
        y=0
        #print(f'number is {number}')
        #print(f'y is {y}')
    elif number>16:
        break
